<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbj.mapper.OrderMapper">
    <select id="selectBusinessmanByPage" resultType="businessman">
        select id,name,avatar,username,phone,grade,number from businessman limit #{index},#{offset}
    </select>
    <select id="selectDishByPage" resultType="Dish">
        select * from dish where businessman_id=#{businessmanId} limit #{index},#{offset}
    </select>
</mapper>